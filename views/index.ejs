<body id="login-page">

<div class="form-box">
  <div class="form-logo">
    <p class="text-center"><img src="img/logo.png" alt="logo"></p>
  </div>
  <form action="/login" method="post">
    <input type="text" class="form-control" placeholder="Username" name="username">
    <input type="password" class="form-control" placeholder="Password" name="password">
    <input type="checkbox"> Save Password
    <input type="submit" class="btn btn-social" value="LOGIN">
  </form>
  <a href="#" class="login-action" title="Forgot password">
    <p>Forgot your password?</p>
  </a>
  <a href="/signup" class="login-action" title="Signi in">
    <p>New User?</p>
  </a>
</div><!--Close form-box-->

<footer class="login-footer text-center">
  <div class="col-xs-3">
    <a class="login-action" href="#" title="language">LANGUAGE</a>
  </div>
  <div class="col-xs-3">
    <a class="login-action" href="#" title="terms">TERMS</a>
  </div>
  <div class="col-xs-3">
