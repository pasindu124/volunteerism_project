                    </li>
                    <li>
                        <a href="/admin/profile" class="waves-effect"><i class="fa fa-user m-r-10" aria-hidden="true"></i>Profile</a>
                    </li>
                    <li>
                        <a href="/admin/events" class="waves-effect"><i class="fa fa-plus-square m-r-10" aria-hidden="true"></i>Events</a>
                    </li>

                </ul>
                <div class="text-center m-t-30">
                    <a href="https://wrappixel.com/templates/monsteradmin/" class="btn btn-danger"> Visit to website</a>
                </div>
            </nav>
            <!-- End Sidebar navigation -->
